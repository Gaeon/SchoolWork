package assignment;

public class OperationEx2 {

	public static void main(String[] args) {

		int num = 10;
		System.out.println(num);
		System.out.println(++num);
		System.out.println(num);
		System.out.println(num++);
		System.out.println(num);
		
		System.out.println(3>5);
		int num1 = 5;
		int num2 = 1;
		boolean flag = (num1 > num2);
		
	}

}
